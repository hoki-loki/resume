{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["../../portfolio/resume-portfolio/src/**/*.ts", "../../portfolio/resume-portfolio/src/**/*.d.ts", "../../portfolio/resume-portfolio/src/**/*.tsx", "../../portfolio/resume-portfolio/src/**/*.vue"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
